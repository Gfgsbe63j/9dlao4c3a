function readKey()
    local savedKey = readfile("XeyHub/key.txt")
    if savedKey then
        print("Saved key")
        savedKey = savedKey:match("^%s*(.-)%s*$")
    end
    if not savedKey or savedKey == "" then
        OrionLib:MakeNotification({
            Name = "Key not found in local storage",
            Content = "We may have updated the key, please re-enter get a new key.",
            Time = 5
        })
        return
    end

    getgenv().userinput = savedKey
    if savedKey then 
        if table.find(getbetter, savedKey) then 
            OrionLib:MakeNotification({
            Name = "Script is loading",
            Content = "Ty for using our service",
            Time = 5
        })
        G2L["1"]:Destroy()
        ExecuteScript()
        end
        checkKey()
    end

end

function checkKey()
    local userInput = getgenv().userinput
    if table.find(getbetter, userInput) then
        writefile("XeyHub/key.txt", userInput)
        print(userInput)
        OrionLib:MakeNotification({
            Name = "Correct Key!",
            Content = "Executing the script...",
            Time = 5
        })
        G2L["1"]:Destroy()
        ExecuteScript()
        wait(3)
    elseif not table.find(getbetter, userInput) then

        OrionLib:MakeNotification({
            Name = "Incorrect Key!",
            Content = "You have entered a wrong key! or You didn't press enter after entering the key.",
            Time = 5
        })

    end
end

